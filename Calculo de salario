#include <iostream>

using namespace std;

int main()
{
    float d,j,s;
    cout<<"Ingresar la cantidad de dias trabajados"<< endl;
    cin>>d;
    cout<< "Ganancias por dias"<< endl;
    cin>> j;
    s=d*j;
    cout<<"Trabaje flojo, Su salario es:"<< s << endl;

    return 0;
}
